{
    "name": "lazyants/workflow-bundle",
    "type": "symfony-bundle",
    "description": "Workflow Bundle provides basic usefully functional for workflows",
    "keywords": ["workflow", "bundle"],
    "homepage": "https://github.com/lazyants/WorkflowBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Spivakovsky",
            "email": "maxim@lazy-ants.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.1,<=3.0"
    },
    "require-dev": {
        "doctrine/orm": ">=2.2,<2.5-dev"
    },
    "autoload": {
        "psr-0": {
            "Lazyants\\WorkflowBundle": ""
        }
    },
    "target-dir": "Lazyants/WorkflowBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}